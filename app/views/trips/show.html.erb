<h1><%= @trip.title %></h1>
<h2><%= @trip.destination %></h2>
<h2><%= @trip.mileage %></h2>
<h3>Travelers:</h3>
<% @trip.travelers.each do |traveler| %>
<section id="traveler-<%=traveler.id%>">
  <p><%=traveler.name%></p><%= button_to "Remove Traveler", "/trips/#{@trip.id}/travelers/#{traveler.id}", method: :delete %>
</section>
<% end %>